    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "jest": "^22.1.4",
    "jest-json-schema": "^1.2.0",
    "sequelize-cli": "^3.2.0",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "hapi": "^16.6.2",
    "hapi-swagger": "8.0.0",
    "inert": "^4.2.1",
    "joi": "^9.2.0",
